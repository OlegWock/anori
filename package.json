{
    "name": "Anori",
    "description": "Customizable new tab: compose your own page from widgets",
    "version": "1.24",
    "type": "module",
    "repository": "git@github.com:OlegWock/anori.git",
    "author": "OlegWock",
    "license": "AGPL-3.0",
    "scripts": {
        "postinstall": "patch-package",
        "dev": "cross-env NODE_OPTIONS='--loader ts-node/esm --disable-warning=ExperimentalWarning' rspack build --mode development",
        "production": "cross-env NODE_OPTIONS='--loader ts-node/esm --disable-warning=ExperimentalWarning' rspack build --mode production",
        "dev:rsdoctor": "cross-env NODE_OPTIONS='--loader ts-node/esm --disable-warning=ExperimentalWarning' RSDOCTOR='true' rspack build --mode development",
        "production:rsdoctor": "cross-env NODE_OPTIONS='--loader ts-node/esm --disable-warning=ExperimentalWarning' RSDOCTOR='true' rspack build --mode production",
        "production:all-permissions": "cross-env NODE_OPTIONS='--loader ts-node/esm --disable-warning=ExperimentalWarning' rspack build --mode production --env targetBrowser=chrome-all-permissions",
        "watch": "cross-env NODE_OPTIONS='--loader ts-node/esm --disable-warning=ExperimentalWarning' rspack build --mode development --watch",
        "dev:ff": "cross-env NODE_OPTIONS='--loader ts-node/esm --disable-warning=ExperimentalWarning' rspack build --mode development --env targetBrowser=firefox",
        "production:ff": "cross-env NODE_OPTIONS='--loader ts-node/esm --disable-warning=ExperimentalWarning' rspack build --mode production --env targetBrowser=firefox",
        "watch:ff": "cross-env NODE_OPTIONS='--loader ts-node/esm --disable-warning=ExperimentalWarning' rspack build --mode development --watch --env targetBrowser=firefox",
        "prepare": "husky install",
        "format": "biome format --write .",
        "lint": "biome lint --write .",
        "biome:check": "biome check --write .",
        "typecheck": "tsc --noEmit -p tsconfig.json",
        "translations:missing": "ts-node translations-manager.ts check-missing",
        "translations:extract": "ts-node translations-manager.ts extract-untranslated",
        "translations:clean": "ts-node translations-manager.ts remove-excessive",
        "translations:merge": "ts-node translations-manager.ts merge-back",
        "translations:locales": "ts-node translations-manager.ts generate-locales"
    },
    "lint-staged": {
        "*": ["biome check --no-errors-on-unmatched --files-ignore-unknown=true --write"]
    },
    "dependencies": {
        "@biomejs/biome": "^1.9.4",
        "@floating-ui/react": "^0.27.16",
        "@iconify/json": "^2.2.341",
        "@iconify/react": "^6.0.0",
        "@iconify/tools": "^4.1.2",
        "@iconify/types": "^2.0.0",
        "@radix-ui/react-checkbox": "^1.3.2",
        "@radix-ui/react-direction": "^1.1.1",
        "@radix-ui/react-menubar": "^1.1.15",
        "@radix-ui/react-scroll-area": "^1.2.9",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-slider": "^1.3.5",
        "@radix-ui/react-tabs": "^1.1.12",
        "@rsdoctor/rspack-plugin": "^1.1.2",
        "@rspack/cli": "^1.3.12",
        "@rspack/core": "^1.3.12",
        "@svgr/core": "^8.1.0",
        "@svgr/plugin-jsx": "^8.1.0",
        "@tanstack/query-async-storage-persister": "^5.87.4",
        "@tanstack/react-query": "^5.87.4",
        "@tanstack/react-query-persist-client": "^5.87.4",
        "@tanstack/react-virtual": "^3.13.9",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/lodash": "^4.17.17",
        "@types/node": "^22.15.21",
        "@types/react": "^19.1.5",
        "@types/react-dom": "^19.1.5",
        "@types/react-window": "^1.8.5",
        "@types/webextension-polyfill": "^0.10.0",
        "@uiw/react-color": "^2.9.0",
        "clsx": "^2.1.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "file-loader": "^6.2.0",
        "filemanager-webpack-plugin": "^7.0.0",
        "framer-motion": "^12.17.0",
        "generate-file-webpack-plugin": "^1.0.1",
        "husky": "^9.1.7",
        "i18next": "^25.2.1",
        "jotai": "^2.12.4",
        "jszip": "^3.10.1",
        "lint-staged": "^16.0.0",
        "lodash": "^4.17.21",
        "mathjs": "^14.5.0",
        "moment": "^2.30.1",
        "moment-locales-webpack-plugin": "^1.2.0",
        "moment-timezone": "^0.6.0",
        "moment-timezone-data-webpack-plugin": "^1.5.1",
        "patch-package": "^8.0.0",
        "react": "^19.1.0",
        "react-devtools": "^6.1.5",
        "react-devtools-core": "^6.1.5",
        "react-dom": "^19.1.0",
        "react-hotkeys-hook": "^4.3.7",
        "react-i18next": "^15.5.2",
        "react-markdown": "^10.1.0",
        "react-merge-refs": "^3.0.2",
        "react-textarea-autosize": "^8.5.9",
        "react-use-motion-measure": "^3.0.0",
        "react-window": "^1.8.11",
        "remark-breaks": "^4.0.0",
        "remark-gfm": "^4.0.1",
        "rss-parser": "^3.13.0",
        "sass": "^1.93.2",
        "sass-loader": "^16.0.6",
        "stream-browserify": "^3.0.0",
        "string-replace-loader": "^3.1.0",
        "style-loader": "^3.3.3",
        "timers-browserify": "^2.0.12",
        "to-string-loader": "^1.2.0",
        "ts-checker-rspack-plugin": "^1.1.5",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2",
        "unplugin-icons": "^22.3.0",
        "url": "^0.11.0",
        "walk-sync": "^3.0.0",
        "webext-detect": "^5.3.2",
        "webextension-polyfill": "^0.12.0",
        "webpack": "^5.101.3",
        "webpack-target-webextension": "^2.1.3"
    }
}

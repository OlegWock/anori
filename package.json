        "@types/react-grid-layout": "^1.3.2",
        "@types/react-window": "^1.8.5",
        "@types/webextension-polyfill": "^0.10.0",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "babel-loader": "^8.2.5",
        "clean-webpack-plugin": "^4.0.0",
        "clsx": "^1.2.1",
